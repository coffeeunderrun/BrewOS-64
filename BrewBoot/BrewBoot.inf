[Defines]
  INF_VERSION               = 0x00010005
  BASE_NAME                 = BrewBoot
  FILE_GUID                 = ab39b880-5c95-11e9-8d98-9cb6d0bcdce0
  MODULE_TYPE               = UEFI_APPLICATION
  VERSION_STRING            = 1.0
  ENTRY_POINT               = brew_boot_main

[Packages]
  MdePkg/MdePkg.dec

[Sources]
  BrewBoot.c
  Pe.c

[LibraryClasses]
  BaseLib
  BaseMemoryLib
  DebugLib
  PeCoffGetEntryPointLib
  PeCoffLib
  UefiApplicationEntryPoint
  UefiLib

[Guids]
  gEfiFileInfoGuid

[Protocols]
  gEfiLoadedImageProtocolGuid
  gEfiSimpleFileSystemProtocolGuid

[BuildOptions]
  *_*_*_DLINK_FLAGS = /ALIGN:4096 /FILEALIGN:4096 /SUBSYSTEM:CONSOLE

  DEBUG_*_*_CC_FLAGS = /Od /DIS_DEBUG
  DEBUG_*_*_DLINK_FLAGS = /EXPORT:InitializeDriver=$(IMAGE_ENTRY_POINT) /DEBUG $(OUTPUT_PATH)/kernel.lib